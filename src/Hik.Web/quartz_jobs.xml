                    <entry>
                        <key>Job</key>
                        <value>Job.Impl.HikVideoDownloaderJob, Job</value>
                    </entry>
                    <entry>
                        <key>ConfigPath</key>
                        <value>street.json</value>
                    </entry>
                    <entry>
                         <key>RunAsTask</key>
                         <value>false</value>
                    </entry>
                </job-data-map>
                <misfire-instruction>DoNothing</misfire-instruction>
                <cron-expression>0 0/30 * * * ?</cron-expression>
