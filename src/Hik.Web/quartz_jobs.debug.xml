<?xml version="1.0" encoding="utf-16"?>
<job-scheduling-data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="2.0" xmlns="http://quartznet.sourceforge.net/JobSchedulingData">
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>
  <schedule>
    <job>
      <name>Launcher</name>
      <group />
      <description />
      <job-type>Hik.Web.Scheduler.CronTrigger, Hik.Web</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>Floor1_Photo</name>
        <group>JobHost</group>
        <description>Floor1. Move photos from c:\\ftp to cloud</description>
        <job-name>Launcher</job-name>
        <job-group />
        <job-data-map>
          <entry>
            <key>Job</key>
            <value>Job.Impl.ArchiveJob, Job</value>
          </entry>
          <entry>
            <key>ConfigPath</key>
            <value>floor1_photo.json</value>
          </entry>
          <entry>
            <key>RunAsTask</key>
            <value>true</value>
          </entry>
        </job-data-map>
        <misfire-instruction>DoNothing</misfire-instruction>
        <cron-expression>0 2 0/1 * * ?</cron-expression>
      </cron>
    </trigger>
    <trigger>
      <cron>
        <name>Outdoor_Video</name>
        <group>JobHost</group>
        <description>Delete oldest files if empty space less than 10%</description>
        <job-name>Launcher</job-name>
        <job-group />
        <job-data-map>
          <entry>
            <key>Job</key>
            <value>Job.Impl.GarbageCollectorJob, Job</value>
          </entry>
          <entry>
            <key>ConfigPath</key>
            <value>none.json</value>
          </entry>
          <entry>
            <key>RunAsTask</key>
            <value>true</value>
          </entry>
        </job-data-map>
        <misfire-instruction>DoNothing</misfire-instruction>
        <cron-expression>0 56 0/1 * * ?</cron-expression>
      </cron>
    </trigger>
    <trigger>
      <cron>
        <name>NDaysCleanJob</name>
        <group>JobHost</group>
        <description>Delete oldest files if empty space less than 10%</description>
        <job-name>Launcher</job-name>
        <job-group />
        <job-data-map>
          <entry>
            <key>Job</key>
            <value>Job.Impl.NDaysCleanJob, Job</value>
          </entry>
          <entry>
            <key>ConfigPath</key>
            <value>nDays.json</value>
          </entry>
          <entry>
            <key>RunAsTask</key>
            <value>true</value>
          </entry>
        </job-data-map>
        <misfire-instruction>DoNothing</misfire-instruction>
        <cron-expression>0 56 0/1 * * ?</cron-expression>
      </cron>
    </trigger>
    <trigger>
      <cron>
        <name>Test</name>
        <group>JobHost</group>
        <description>Test</description>
        <job-name>Launcher</job-name>
        <job-data-map>
          <entry>
            <key>ConfigPath</key>
            <value>etet.json</value>
          </entry>
          <entry>
            <key>RunAsTask</key>
            <value>True</value>
          </entry>
          <entry>
            <key>Job</key>
            <value>ghj</value>
          </entry>
        </job-data-map>
        <misfire-instruction>DoNothing</misfire-instruction>
        <cron-expression>0 50 0/1 * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
</job-scheduling-data>