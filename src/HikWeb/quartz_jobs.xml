<?xml version="1.0" encoding="UTF-8"?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
  
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    <job>
      <name>HikDownloader</name>
      <group>DEFAULT</group>
      <description>Download files from Hikvision Cameras</description>
      <job-type>HikWeb.Job.HikDownloaderJob, HikWeb</job-type>
      <durable>true</durable>
      <recover>false</recover>
      <job-data-map>
        <entry>
          <key>key0</key><value>value0</value>
        </entry>
      </job-data-map>
    </job>
    <trigger>
      <cron>
        <name>HikDownloaderTrigger</name>
        <job-name>HikDownloader</job-name>
        <cron-expression>0 0/30 * 1/1 * ? *</cron-expression>
      </cron>
    </trigger>
    
    <job>
      <name>DeleteArchivingJob</name>
      <group>DEFAULT</group>
      <description>Download files from Hikvision Cameras</description>
      <job-type>HikWeb.Job.DeleteArchivingJob, HikWeb</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>DeleteArchivingJobTrigger</name>
        <job-name>DeleteArchivingJob</job-name>
        <cron-expression>0 15 4 1/1 * ? *</cron-expression>
      </cron>
    </trigger>
  </schedule>

</job-scheduling-data>