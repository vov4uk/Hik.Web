<?xml version="1.0" encoding="UTF-8"?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
  
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    <job>
      <name>Launcher</name>
      <group></group>
      <description></description>
      <job-type>HikWeb.CronTrigger, HikWeb</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <trigger>
      <cron>
        <name>Street</name>
        <group></group>
        <description>Download videos from street camera</description>
        <job-name>Launcher</job-name>
        <job-group></job-group>
        <job-data-map>
          <entry>
            <key>Job</key>
            <value>Job.Impl.HikDownloaderJob, Job</value>
          </entry>
          <entry>
            <key>JobType</key>
            <value>HikDownloader</value>
          </entry>
          <entry>
            <key>ConfigPath</key>
            <value>configuration.street.json</value>
          </entry>
        </job-data-map>
        <misfire-instruction>DoNothing</misfire-instruction>
        <cron-expression>0 0/1 * * * ?</cron-expression>
      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>Internal</name>
        <group></group>
        <description>Download videos from internal camera</description>
        <job-name>Launcher</job-name>
        <job-group></job-group>
        <job-data-map>
          <entry>
            <key>Job</key>
            <value>Job.Impl.HikDownloaderJob, Job</value>
          </entry>
          <entry>
            <key>JobType</key>
            <value>HikDownloader</value>
          </entry>
          <entry>
            <key>ConfigPath</key>
            <value>configuration.internal.json</value>
          </entry>
        </job-data-map>
        <misfire-instruction>DoNothing</misfire-instruction>
        <cron-expression>0 0/1 * * * ?</cron-expression>
      </cron>
    </trigger>
    
    <trigger>
      <cron>
        <name>DeleteArchiving</name>
        <group></group>
        <description>Delete videos</description>
        <job-name>Launcher</job-name>
        <job-group></job-group>
        <job-data-map>
          <entry>
            <key>Job</key>
            <value>Job.Impl.DeleteArchivingJob, Job</value>
          </entry>
          <entry>
            <key>JobType</key>
            <value>DeleteArchiving</value>
          </entry>
          <entry>
            <key>ConfigPath</key>
            <value>configuration.json</value>
          </entry>
        </job-data-map>
        <misfire-instruction>DoNothing</misfire-instruction>
        <cron-expression>0 0/5 * * * ?</cron-expression>
      </cron>
    </trigger>

  </schedule>

</job-scheduling-data>